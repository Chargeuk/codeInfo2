PORT=5010
# Adjust CORS origins if needed; use .env.local for private overrides
LMSTUDIO_BASE_URL=http://host.docker.internal:1234
CHROMA_URL=http://localhost:8000
INGEST_EXCLUDE=node_modules,.git,dist,build,coverage,logs,vendor,*.log,*.min.js,package-lock.json,yarn.lock,pnpm-lock.yaml
INGEST_INCLUDE=ts,tsx,js,jsx,mjs,cjs,json,jsonc,md,mdx,txt,py,java,kt,kts,go,rs,rb,php,cs,cpp,cc,c,h,hpp,swift,scala,clj,cljs,edn,sh,bash,zsh,ps1,yaml,yml,toml,ini,cfg,env,sql
LOG_LEVEL=info
LOG_BUFFER_MAX=5000
LOG_MAX_CLIENT_BYTES=32768
LOG_FILE_PATH=/app/logs/server.log
LOG_FILE_ROTATE=true
